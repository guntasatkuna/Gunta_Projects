public class Superhero {
}
    public static void main(String[] args) {
        // write your code here

     Write a Java program which does provide information about you,
     and print that on the screen,
     and push that into your personal GitHub project.
     E.g.:
     - initiates variables:
     name (String type)
     age (int type)
     gender (char type)

     */

    String nameOfYou;
    short ageOfYou;
    boolean isPregnant;
    int daysPregnant;
    double weeksPregnant;
    float weightOfYou;
    byte numberOfTrimesterNow;
    byte daysInWeek;
    String Trimester1, Trimester2, Trimester3;

    nameOfYou = "Gunta";
    ageOfYou = 30;
    weightOfYou = 70F;
    isPregnant = true;
    daysPregnant = 215;
    daysInWeek = 7;
    weeksPregnant = (double)daysPregnant/ daysInWeek;
    numberOfTrimesterNow = 3;

    Trimester1 = "Super excited, but feeling tired and sick.";
    Trimester2 = "Feeling perfect, happy and excited about the upcoming Maternity leave! :) ";
    Trimester3 = "Still happy but physically feeling very bad because of the hot weather.  ";

    System.out.println("************************************************************");
    System.out.println();
    System.out.println("Your name: ");
    System.out.println(nameOfYou);
    System.out.print("Your age: ");
    System.out.println(ageOfYou);
    System.out.println("Is Pregnant: ");
    System.out.println(isPregnant);
    System.out.println("Days Pregnant: ");
    System.out.println(daysPregnant);
    System.out.println("Weeks Pregnant: ");
    System.out.println(weeksPregnant);
    System.out.println("Trimester No: ");
    System.out.println(numberOfTrimesterNow);
    System.out.println("Your weight: ");
    System.out.println(weightOfYou);

    System.out.println("------------");
    System.out.println();
    System.out.println("Your feelings during pregnancy:");
    System.out.println("Trimester No.1: ");
    System.out.println(Trimester1);
    System.out.println("Trimester No.2: ");
    System.out.println(Trimester2);
    System.out.println("Trimester No.3: ");
    System.out.println(Trimester3);

    System.out.println("------------");

    System.out.print("? -What is her status? ");
    System.out.println(isPregnant);
    System.out.print("Trimester (1 - 3): ");
    System.out.println(numberOfTrimesterNow);
    System.out.println("************************************************************");
}
